{
    "name": "vscode-epm",
    "displayName": "vscode-epm",
    "description": "EPM syntax highlighting for VScode",
    "version": "0.0.2",
    "publisher": "smhc",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/smhc/vscode-epm.git"
      }, 
    "contributes": {
        "languages": [{
            "id": "epm",
            "aliases": ["epm", "epm"],
            "extensions": [".eth"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "epm",
            "scopeName": "source.epm",
            "path": "./syntaxes/epm.tmLanguage.json"
        }]
    }
}